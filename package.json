{
  "name": "ts-http-status-classes",
  "version": "1.0.0",
  "description": "Typescript Classes representing HTTP states",
  "exports": {
    "./base": {
      "types": "./dist/base/index.d.ts",
      "import": "./dist/base/index.js",
      "require": "./dist/base/index.cjs"
    },
    "./http-infos": {
      "types": "./dist/http-infos/index.d.ts",
      "import": "./dist/http-infos/index.js",
      "require": "./dist/http-infos/index.cjs"
    },
    "./http-successess": {
      "types": "./dist/http-successess/index.d.ts",
      "import": "./dist/http-successess/index.js",
      "require": "./dist/http-successess/index.cjs" 
    },
    "./http-redirects": {
      "types": "./dist/http-redirects/index.d.ts",
      "import": "./dist/http-redirects/index.js",
      "require": "./dist/http-redirects/index.cjs"
    },
    "./http-server-errors": {
      "types": "./dist/http-server-errors/index.d.ts",
      "import": "./dist/http-server-errors/index.js",
      "require": "./dist/http-server-errors/index.cjs"
      
    },
    "./http-client-errors": {
      "types": "./dist/http-client-errors/index.d.ts",
      "import": "./dist/http-client-errors/index.js",
      "require": "./dist/http-client-errors/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup ./src/base/index.ts ./src/http-infos/index.ts ./src/http-successess/index.ts ./src/http-redirects/index.ts ./src/http-server-errors/index.ts ./src/http-client-errors/index.ts --format esm,cjs --dts --out-dir dist",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "@lordcraymen/ts-http-status-classes"
  },
  "keywords": [
    "http",
    "status",
    "classes"
  ],
  "author": "Florian \"lordcraymen\" Patzke",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
